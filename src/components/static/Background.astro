---
import background from "@/assets/background.svg";
---

<img id="background" src={background.src} alt="" fetchpriority="high" style="animation: pulse 7s cubic-bezier(0.4, 0, 0.6, 1) infinite;" />
<style>
    #background {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -1;
        filter: blur(100px);
    }
</style>
